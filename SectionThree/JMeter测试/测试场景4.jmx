        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="jp@gc - WebDriver Sampler">
          <stringProp name="WebDriverSampler.script">import org.openqa.selenium.By
import org.openqa.selenium.support.ui.Select
import org.openqa.selenium.Dimension

WDS.sampleResult.sampleStart()

// 打开首页并设置窗口大小
WDS.browser.get(&quot;http://127.0.0.1:63735&quot;)
WDS.browser.manage().window().setSize(new Dimension(2576, 1408))

// 选择货币为 JPY
def currencyDropdown = WDS.browser.findElement(By.name(&quot;currency_code&quot;))
currencyDropdown.click()
new Select(currencyDropdown).selectByVisibleText(&quot;JPY&quot;)

// 点击第5个热销商品
WDS.browser.findElement(By.cssSelector(&quot;.col-md-4:nth-child(5) .hot-product-card-img-overlay&quot;)).click()

// 设置商品数量为5
def quantityDropdown = WDS.browser.findElement(By.id(&quot;quantity&quot;))
quantityDropdown.click()
new Select(quantityDropdown).selectByVisibleText(&quot;5&quot;)

// 加入购物车
WDS.browser.findElement(By.cssSelector(&quot;.cymbal-button-primary&quot;)).click()

// 点击结账
WDS.browser.findElement(By.cssSelector(&quot;.cymbal-button-primary:nth-child(1)&quot;)).click()

// 点击“Continue Shopping”
WDS.browser.findElement(By.linkText(&quot;Continue Shopping&quot;)).click()

WDS.sampleResult.sampleEnd()

WDS.log.info(&quot;脚本 Test04 执行完成&quot;)</stringProp>
